  <Template id="JET_Accrued_Payroll" name="Monthly Payroll Accrual" active="true">
    <JEType>ADJUST</JEType>
    <Frequency>Monthly</Frequency>
    <Reversing>true</Reversing>
    <Description>Accrue estimated payroll costs for the period. Reverses on first day of next month when actual payroll posts.</Description>
    <RequiredApproval>PlantController</RequiredApproval>
    <AccountMapping>
      <DebitAccount code="SGA_Salaries" description="SGA Salaries and Wages" />
      <CreditAccount code="ACCR_Payroll" description="Accrued Payroll" />
    </AccountMapping>
    <DefaultAmount>0</DefaultAmount>
    <ApplicableEntities>
      <Entity code="Plant_US01_Detroit" />
      <Entity code="Plant_US02_Houston" />
      <Entity code="Plant_US03_Charlotte" />
      <Entity code="Plant_CA01_Toronto" />
      <Entity code="Plant_MX01_Monterrey" />
      <Entity code="Plant_DE01_Munich" />
      <Entity code="Plant_DE02_Stuttgart" />
      <Entity code="Plant_UK01_Birmingham" />
      <Entity code="Plant_FR01_Lyon" />
      <Entity code="Plant_CN01_Shanghai" />
      <Entity code="Plant_CN02_Shenzhen" />
      <Entity code="Plant_JP01_Osaka" />
      <Entity code="Plant_IN01_Pune" />
    </ApplicableEntities>
    <TemplateFile>Templates/JET_Accrued_Payroll.csv</TemplateFile>
  </Template>

