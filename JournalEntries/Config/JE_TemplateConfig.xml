  <!-- INTERCOMPANY ELIMINATION TEMPLATES (ELIM type)               -->
  <!-- ============================================================ -->

  <Template id="JET_IC_Revenue_COGS" name="IC Revenue/COGS Elimination" active="true">
    <JEType>ELIM</JEType>
    <Frequency>Monthly</Frequency>
    <Reversing>false</Reversing>
    <Description>Eliminate intercompany transfer sales revenue against purchasing entity cost of goods sold. Includes service revenue and royalty eliminations.</Description>
    <RequiredApproval>RegionalController</RequiredApproval>
    <AccountMapping>
      <DebitAccount code="REV_IC_TransferSales" description="IC Transfer Sales" />
      <CreditAccount code="RawMaterials" description="Raw Materials Consumed" />
      <AlternateDebit code="REV_IC_ServiceRevenue" description="IC Service Revenue" />
      <AlternateCredit code="SGA_Professional" description="Professional Services" />
    </AccountMapping>
    <DefaultAmount>0</DefaultAmount>
    <ApplicableEntities>
      <Entity code="Elim_Americas" />
      <Entity code="Elim_EMEA" />
      <Entity code="Elim_APAC" />
      <Entity code="Elim_CrossRegion" />
    </ApplicableEntities>
    <TemplateFile>Templates/JET_IC_Revenue_COGS.csv</TemplateFile>
  </Template>

