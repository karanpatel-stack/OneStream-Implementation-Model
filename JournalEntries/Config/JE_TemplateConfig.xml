<?xml version="1.0" encoding="UTF-8"?>
<!--
  JE Template Configuration
  Defines all journal entry templates with metadata, account mappings, and applicable entities.
  Used by FR_JournalEntries.vb to validate and process template-based journal entries.
-->
<JETemplateConfig>

  <!-- ============================================================ -->
  <!-- MONTHLY ACCRUAL TEMPLATES (ADJUST type)                      -->
